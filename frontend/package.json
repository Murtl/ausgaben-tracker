{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "build-only": "vite build --emptyOutDir",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-path .eslintignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "chart.js": "^4.2.1",
    "navigator-languages": "^2.0.2",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "json-d-ts": "^1.0.1",
    "prettier": "^2.8.4",
    "sass": "^1.60.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-minify": "^1.5.2",
    "vue-tsc": "^1.2.0"
  }
}
